{
  "cleanUrls": true,
  "headers": [
    {
      "source": "/.well-known/webfinger",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/jrd+json"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/files/:match*",
      "destination": "http://files.iansutherland.ca/:match*"
    },
    {
      "source": "/keybase.txt",
      "destination": "http://files.iansutherland.ca/keybase.txt"
    }
  ]
}
