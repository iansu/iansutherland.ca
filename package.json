{
  "name": "iansutherland.ca",
  "description": "Personal site of Ian Sutherland",
  "version": "2.0.0",
  "author": "Ian Sutherland",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/iansu/iansutherland.ca"
  },
  "scripts": {
    "start": "gatsby develop",
    "start:production": "serve -s public",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "format": "prettier --write \"**/*.{ts,js,json,graphql,gql,yaml,yml,md}\"",
    "format:check": "prettier --debug-check \"**/*.{ts,js,json,graphql,gql,yaml,yml,md}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "concurrent": false,
    "linters": {
      "*.{js,json,graphql,gql,yaml,yml,md}": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby": "^2.3.25",
    "gatsby-plugin-google-analytics": "^2.0.20",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-react-svg": "^2.1.1",
    "gatsby-plugin-styled-components": "^3.0.7",
    "normalize.css": "^8.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "serve": "^11.0.0"
  }
}
