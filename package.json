{
  "name": "iansutherland.ca-next",
  "description": "Personal site of Ian Sutherland",
  "version": "2.1.0",
  "author": "Ian Sutherland",
  "license": "MIT",
  "private": true,
  "homepage": "https://iansutherland.ca",
  "repository": {
    "type": "git",
    "url": "https://github.com/iansu/iansutherland.ca"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "start": "next dev",
    "start:production": "next start",
    "build": "next build",
    "storybook": "start-storybook -s ./public -p 6006",
    "storybook:build": "build-storybook",
    "clean": "rimraf .next",
    "format": "prettier --write \"**/*.{ts,tsx,js,json,yaml,yml,md}\"",
    "format:check": "prettier --debug-check \"**/*.{ts,tsx,js,json,yaml,yml,md}\"",
    "prepare": "husky install",
    "precommit": "pretty-quick --staged"
  },
  "dependencies": {
    "next": "^10.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.2.9",
    "@types/react": "^17.0.5",
    "@types/styled-components": "^5.1.9",
    "autoprefixer": "^10.2.5",
    "husky": "^6.0.0",
    "postcss": "^8.2.13",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.2.4"
  }
}
